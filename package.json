  "private": true,
  "type": "module",
  "scripts": {
   "dev": "vite",
    "dev:server": "node --inspect dist/server.js",
    "build": "tsc && vite build",
    "build:backend": "tsc -p tsconfig.server.json",
    "render-build": "npm run build && npm run build:backend",
    "start": "NODE_ENV=production node dist/server.js",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "dev:full": "npm run build:backend && concurrently \"npm run dev\" \"npm run dev:server\""
  },
